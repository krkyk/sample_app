<div class="list-wrap">
 <%@lists.each do |list|%>
  <div class="list-box">
   <%if list.list_image.attached?%>
    <%=image_tag list.list_image,class:"icon"%>
   <%else%>
    <%=image_tag"no_image",class:"icon"%>
   <%end%>
   <div class="list-comment">
    <span class="list-body"><%=list.body%></span>
    <span class="list-title"><%=link_to list.title,list_path(list.id)%></span>
   </div>
  </div>
 <%end%>
</div>
